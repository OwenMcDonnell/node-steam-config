{
    "plugins": [
      "plugins/markdown"
    ],
    "source": {
        "include": [
          "./lib/index.js",
          "./lib/steam-paths.js",
          "./lib/steamdata-utils.js",
          "./lib/steam-settings.js"
        ]
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "referenceTitle": "SteamConfig API Reference",
        "disableSort": true,
        "monospaceLinks": true,
        "default": {
          "includeDate": false,
          "outputSourceFiles": false
        },
        "navType": "vertical",
        "linenums": true,
        "collapseSymbols": true,
        "inverseNav": true,
        "includeSource": false
    },
    "opts": {
      "destination": "./docs",
      "encoding": "utf8",
      "recurse": false,
      "sort": false,
      "access": "all"
    }
}
